{
  "entrypoint": "index.html",
  "shell": "src/components/my-app.js",
  "sources": [
    "images/**/*"
  ],
  "extraDependencies": [
    "node_modules/@webcomponents/webcomponentsjs/**",
    "manifest.webmanifest",
    "firebase-messaging-sw.js",
    "browserconfig.xml",
    "crossdomain.xml"
  ],
  "builds": [
    {
      "name": "esm-unbundled",
      "browserCapabilities": [
        "es2015",
        "modules",
        "push"
      ],
      "js": { "minify": true },
      "css": { "minify": true },
      "html": { "minify": true },
      "bundle": false,
      "addServiceWorker": true
    },
    {
      "name": "es6-bundled",
      "browserCapabilities": [
        "es2015"
      ],
      "js": {
        "compile": "es2015",
        "minify": true,
        "transformModulesToAmd": true
      },
      "css": { "minify": true },
      "html": { "minify": true },
      "bundle": true,
      "addServiceWorker": true
    },
    {
      "name": "es5-bundled",
      "js": {
        "compile": "es5",
        "minify": true,
        "transformModulesToAmd": true
      },
      "css": { "minify": true },
      "html": { "minify": true },
      "bundle": true,
      "addServiceWorker": true
    }
  ],
  "moduleResolution": "node",
  "npm": true
}
